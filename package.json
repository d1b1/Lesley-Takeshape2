{
  "name": "takeshape-plain",
  "version": "1.0.0",
  "scripts": {
    "init": "tsg init",
    "serveSite": "browser-sync start --server build --port 5000 --files 'build/**/*' --reload-debounce",
    "build": "tsg build -c",
    "deploy": "tsg deploy",
    "watch:tsg": "tsg watch -W -c",
    "watch": "npm-run-all --serial build && npm-run-all --continue-on-error --parallel watch:* serveSite",
    "start": "npm run watch"
  },
  "dependencies": {
    "browser-sync": "2.26.7",
    "eslint": "6.5.1",
    "npm-run-all": "4.1.5",
    "servedir": "1.0.1",
    "stylelint": "11.0.0",
    "stylelint-config-recommended": "3.0.0",
    "takeshape-cli": "4.97.0"
  },
  "keywords": ["kkkkkkkk"],
  "main": "index.js",
  "license": "MIT"
}
